library(ggplot2)
gapminder <- read.csv("gapminder.csv")
ggplot(gapminder[gapminder$country == "United States",], aes(x = year)) +
geom_line(aes(y = lifeExp, color = "Life Expectancy")) +
geom_line(aes(y = gdpPercap/1000, color = "GDP per Capita (in thousands)")) +
geom_line(aes(y = pop/1000000, color = "Population (in millions)")) +
labs(title = "Changes in Life Expectancy, GDP per Capita, and Population (United States)",
y = "",
color = "Metric")
